#!/usr/bin/env php
<?php

foreach ([__DIR__ . '/../../../autoload.php', __DIR__ . '/../vendor/autoload.php'] as $file) {
    if (file_exists($file)) {
        require $file;
        break;
    }
}

use Pingframework\Boot\Application\ConsolePingBootApplication;

ConsolePingBootApplication::build(false)->run();